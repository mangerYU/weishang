/* pages/index/dailiup/dailiup.wxss */
.user{
  width: 100%;
  height: 200rpx;
  border-bottom: 10rpx solid #eeeeee;
  border-top: 2rpx solid #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 26rpx;
  padding-left: 30rpx;
}
.grade{
  width: 93%;
  height: 140rpx;
  border-bottom: 10rpx solid #eeeeee;
  border-top: 2rpx solid #eeeeee;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 30rpx 0rpx 30rpx;
  font-size: 26rpx;

}
.pzimg{
width: 100%;
height: 170rpx;
display: flex;
flex-direction: row;
align-items: center;
}
.pzimg image{
  width: 160rpx;
  height: 160rpx;
  margin-left: 30rpx;
}
.but{
width: 100%;
height: 120rpx;
border-top: 10rpx solid #eeeeee;
position: absolute;
bottom: 0rpx;
display: flex;
flex-direction: row;
justify-content: flex-end;
align-items: center;
}
.gro{
width: 200rpx;
height: 66rpx;
color: #ffffff;
font-size: 32rpx;
display: flex;
flex-direction: row;
justify-content: center;
align-items: center;

}
.comm{
width: 93%;
}
.row{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
}
.comm image{
width: 104rpx;
height: 104rpx;
margin-right:16rpx;
}
.num{
  font-size: 28rpx;
  width: 90%;
  padding: 0rpx 35rpx;
  margin-top: 50rpx;
}
.column{
display: flex;
flex-direction: column;
justify-content: space-around;
}