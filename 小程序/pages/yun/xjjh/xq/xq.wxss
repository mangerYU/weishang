.user{
  width: 100%;
  height: 200rpx;
  border-bottom: 10rpx solid #eeeeee;
  border-top: 2rpx solid #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 26rpx;
  padding-left: 30rpx;
}
.grade{
  width: 93%;
  height: 140rpx;
  border-bottom: 10rpx solid #eeeeee;
  border-top: 2rpx solid #eeeeee;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 30rpx 0rpx 30rpx;
  font-size: 26rpx;

}
.comm{
width: 93%;
}
.row{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
}
.comm image{
width: 104rpx;
height: 104rpx;
margin-right:16rpx;
}
.num{
  font-size:35rpx;
  width: 90%;
  padding: 0rpx 35rpx;
  margin-top: 50rpx;
}
.column{
display: flex;
flex-direction: column;
justify-content: space-around;
}